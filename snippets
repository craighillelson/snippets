  'is today a weekday':
    'prefix': 'weekday'
    'body': '''
    import datetime
    
    def not_weekend():
        """
        Determine if today is a weekday. Return 'weekend' if the date falls
        on a weekend. Return 'not a weekend' if the date does not fall on a
        weekend.
        """

        day_index = datetime.datetime.today().weekday()
        if day_index > 4:
            print('weekend')
        else:
            print('not a weekend')
    '''

  'find last friday\'s date':
    'prefix': 'last friday'
    'body': '''
    from datetime import datetime
    from datetime import timedelta

    weekdays = (
        'Monday',
        'Tuesday',
        'Wednesday',
        'Thursday',
        'Friday',
        'Saturday',
        'Sunday'
    )

    def get_previous_by_day(dayname, start_date=None):
        """Find last Friday's date."""
        if start_date is None:
            start_date = datetime.today()
        day_num = start_date.weekday()
        day_num_target = weekdays.index(dayname)
        days_ago = (7 + day_num - day_num_target) % 7
        if days_ago == 0:
            days_ago = 7
        target_date = start_date - timedelta(days=days_ago)
        return target_date
    '''

  'days from now':
    'prefix': 'determine the date a given number of days from  today\'s date'
    'body': '''
    def days_from_now(a):
        """
        Take today's date and calculate a number of days from same.
        The number of days is specified in the function's argument.
        """

        import datetime

        start_date = datetime.datetime.now() + datetime.timedelta(a)
        return start_date.date()
    '''

