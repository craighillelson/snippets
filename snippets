    def find_date():
        """Find date in a string."""
        import re

        date_num_regex = re.compile(r'\\\\d{4}-\\\\d{2}-\\\\\d{2}')
        mo = date_num_regex.search('I was born on 1989-01-22.')
        print('Date found: ' + mo.group())
    '''

